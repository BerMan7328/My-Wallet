USE mywallet;
GO
CREATE TABLE IF NOT EXISTS `mywallet`.`finance` (
  `id` FLOAT NOT NULL,
  `expenses` FLOAT NULL,
  `gains` FLOAT NULL,
  PRIMARY KEY (`id`));
GO
CREATE TABLE IF NOT EXISTS `mywallet`.`user` (
  `id` FLOAT NOT NULL,
  `username` VARCHAR(255) NOT NULL,
  `email` VARCHAR(255) NULL,
  `password` VARCHAR(32) NOT NULL,
  `status` TINYINT NULL,
  PRIMARY KEY (`id`));
